      <TextField fx:id="nameTextField" layoutX="175.0" layoutY="108.0" />
      <Button fx:id="submitButton" layoutX="224.0" layoutY="241.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <Label fx:id="ageLabel" layoutX="60.0" layoutY="160.0" text="Age:" />
      <Label fx:id="emailLabel" layoutX="58.0" layoutY="192.0" text="Email:" />
